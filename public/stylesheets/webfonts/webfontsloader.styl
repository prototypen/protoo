/* Regular */
@font-face 
	font-family 'League Gothic'
	src url('webfonts/leaguegothic/leaguegothic-italic-webfont.eot')
	src url('webfonts/leaguegothic/leaguegothic-italic-webfont.eot?#iefix') format('embedded-opentype'),
		url('webfonts/leaguegothic/leaguegothic-italic-webfont.woff') format('woff'),
		url('webfonts/leaguegothic/leaguegothic-italic-webfont.ttf') format('truetype'),
		url('webfonts/leaguegothic/leaguegothic-italic-webfont.svg#league_gothic_italicregular') format('svg')
	font-weight normal
	font-style italic



